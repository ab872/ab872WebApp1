            printf ("FIZZ");
         }
          
         if(count % 5 == 0) {     //  divisible by 5
            printf ("BUZZ");
         }
            
         printf ("\n");
           
         // end   for loop
      }
          
      //  end   else
   }

   printf ("End of program.\n");
                    
   //   end     main
}

